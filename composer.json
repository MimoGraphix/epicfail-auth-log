{
    "name": "mimographix/epicfail-auth-log",
    "description": "Customized auth log package",
    "keywords": [
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "MimoGraphix",
            "email": "mimographix@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "illuminate/auth": "~5.5",
        "illuminate/bus": "~5.5",
        "illuminate/console": "~5.5",
        "illuminate/contracts": "~5.5",
        "illuminate/database": "~5.5",
        "illuminate/http": "~5.5",
        "illuminate/notifications": "~5.5",
        "illuminate/support": "~5.5"
    },
    "require-dev": {
        "orchestra/database": "~3.5",
        "orchestra/testbench": "~3.5",
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "Yadahan\\AuthenticationLog\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yadahan\\AuthenticationLog\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Yadahan\\AuthenticationLog\\AuthenticationLogServiceProvider"
            ]
        }
    },
    "suggest": {
        "guzzlehttp/guzzle": "Required to use the Slack transport (~6.0)",
        "nexmo/client": "Required to use the Nexmo transport (~1.0)."
    },
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
